{
  "name": "magic-auth-context",
  "version": "0.2.0",
  "description": "A react context and hook that provide Magic.link functionality across your app.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:ScottyMJacobson/magic-auth-context.git"
  },
  "homepage": "https://github.com/ScottyMJacobson/magic-auth-context#readme",
  "author": "Scotty Jacobson",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/magic-auth-context.esm.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "authentication",
    "context-api",
    "magic",
    "passwordless authentication",
    "magic.link",
    "magic email",
    "react-component"
  ],
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/magic-auth-context.cjs.production.min.js",
      "limit": "35 KB"
    },
    {
      "path": "dist/magic-auth-context.esm.js",
      "limit": "35 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.28",
    "@types/react-dom": "^17.0.9",
    "dts-cli": "^0.19.2",
    "husky": "^7.0.2",
    "promise-deferred": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^6.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "magic-sdk": "^6.1.0",
    "use-deep-compare-effect": "^1.8.1"
  }
}
